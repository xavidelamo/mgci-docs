    <Option name="HELP_URL" type="QString" value=""/>
    <Option name="VegetationDescriptor" type="QString" value="Input Vegetation Descriptor raster"/>
    <Option name="grass7:r.mapcalc.simple_1:Output: Combined Vegetation Mountain Layer" type="QString" value="Outut Combined Vegetation and Mountains raster"/>
  </Option>
